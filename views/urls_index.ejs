<!DOCTYPE html>

<html lang="en">
  <head>
    <title>Tiny App</title>
  </head>

  <body>
    <% include partials/_header.ejs %>
    <% if (user) { %>
    <ul>
      <% for(var key in urls) { %>
      <li>
        <%= key + ": " + urls[key].longURL %>
        <form action="/urls/<%= key %>/delete" method="POST">
        <a href="/urls/<%= key %>">Edit</a>
        <button>Delete</button>
        <br><br>
        </form>
      </li>
      <% } %>
    </ul>
    <% } %>
    <a href="/urls/new">Create new URL</a>
  </body>
</html>