  <h1>TinyApp</h1>

  <% if(user) { %>
    <%= user.email %>
    <form action="/logout" method="POST">
      <input type="submit" value="Logout">
    </form>
  <% } else { %>
    <form action="/login" method="POST">
      <input type="text" name="username">
      <input type="submit" value="Login">
    </form>
  <% } %>